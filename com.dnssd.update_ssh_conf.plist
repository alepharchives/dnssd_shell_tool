<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.dnssd.update_ssh_conf</string>
	<key>Program</key>
	<string>/Full/Path/To/update_ssh_conf.sh</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Full/Path/To/update_ssh_conf.sh</string>
		<string>/Users/Username/.ssh/config</string>
		<string>-w</string>
	</array>
	<key>LimitLoadToSessionType</key>
	<string>Aqua</string>
	<key>KeepAlive</key>
	<true/>
	<key>OnDemand</key>
	<false/>
</dict>
</plist>
